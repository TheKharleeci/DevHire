{
  "name": "devs-for-hire",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text --reporter=text-summary cross-env DEVHIRE_NODE_ENV=test mocha -c -r esm test/**/*.test.js --timeout 12000 --exit",
    "pretest": "npm run lint && cross-env DEVHIRE_NODE_ENV=test npm run migrate:reset && cross-env DEVHIRE_NODE_ENV=test npm run migrate:up && cross-env DEVHIRE_NODE_ENV=test npm run seed:reset && cross-env DEVHIRE_NODE_ENV=test npm run seed:up",
    "prestart": "cross-env DEVHIRE_NODE_ENV=production npm run migrate:up && cross-env DEVHIRE_NODE_ENV=production npm run seed:up",
    "start": "cross-env DEVHIRE_NODE_ENV=production node -r esm .",
    "dev": "nodemon -r esm .",
    "migrate": "db-migrate --migrations-dir ./app/db/migrations",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:reset": "npm run migrate reset",
    "migrate:create": "npm run migrate create",
    "seed:create": "npm run migrate create:seeds",
    "seed:up": "npm run migrate up:seeds",
    "seed:down": "npm run migrate down:seeds",
    "seed:reset": "npm run migrate reset:seeds",
    "lint": "eslint --ext .js ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "bull": "^4.8.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "express-fileupload": "^1.3.1",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.4",
    "pg-monitor": "^1.4.1",
    "pg-promise": "^10.11.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0"
  }
}
